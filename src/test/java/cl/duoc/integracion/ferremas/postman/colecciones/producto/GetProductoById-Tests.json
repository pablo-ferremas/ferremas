{
  "info": {
    "name": "GetProductoById Tests",
    "description": "Colección de pruebas para el endpoint GET /api/v1/productos/{id}",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "01. GET - Obtener Producto por ID Válido",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Accept",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/{{api_version}}/productos/{{valid_producto_id}}",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "{{api_version}}",
            "productos",
            "{{valid_producto_id}}"
          ]
        }
      },
      "response": [],
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Verificar código de estado",
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "// Verificar que la respuesta sea JSON válido",
              "pm.test(\"Response is valid JSON\", function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response).to.be.an('object');",
              "});",
              "",
              "// Verificar tiempo de respuesta",
              "pm.test(\"Response time is less than 5000ms\", function () {",
              "    pm.expect(pm.response.responseTime).to.be.below(5000);",
              "});",
              "",
              "// Verificar headers de respuesta",
              "pm.test(\"Response has correct headers\", function () {",
              "    pm.expect(pm.response.headers.get('Content-Type')).to.include('application/json');",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ]
    },
